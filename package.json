{
  "name": "angular-d3-cloud",
  "version": "1.3.0",
  "scripts": {
    "start": "npm run build:lib && ng serve example",
    "build:lib": "ng build angular-d3-cloud --configuration production",
    "build:example": "ng build example --configuration production",
    "build:all": "npm run build:lib && npm run build:example",
    "update-browserslist": "npx browserslist@latest --update-db",
    "clean-jest": "rimraf .jest-cache coverage",
    "run-jest": "node --expose-gc node_modules/jest/bin/jest.js --coverage --config jest.config.js --logHeapUsage --maxWorkers=30%",
    "run-jest:ci": "node --expose-gc node_modules/jest/bin/jest.js --ci --coverage --config jest.config.js --logHeapUsage",
    "test": "npm run clean-jest && npm run run-jest",
    "test:ci": "npm run clean-jest && npm run run-jest:ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.1",
    "@angular/common": "14.0.1",
    "@angular/compiler": "14.0.1",
    "@angular/core": "14.0.1",
    "@angular/forms": "14.0.1",
    "@angular/platform-browser": "14.0.1",
    "@angular/platform-browser-dynamic": "14.0.1",
    "@angular/router": "14.0.1",
    "d3-cloud": "^1.2.5",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-transition": "^2.0.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.1",
    "@angular/cli": "14.0.1",
    "@angular/compiler-cli": "14.0.1",
    "@types/d3-cloud": "^1.2.3",
    "@types/d3-scale": "^3.2.2",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-transition": "^2.0.0",
    "@types/jest": "28.1.1",
    "@types/node": "14.18.2",
    "codelyzer": "^6.0.0",
    "jest": "28.1.1",
    "jest-canvas-mock": "2.4.0",
    "jest-preset-angular": "12.1.0",
    "ng-packagr": "14.0.2",
    "rimraf": "3.0.2",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  }
}
